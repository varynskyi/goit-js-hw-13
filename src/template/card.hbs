{{#each this}}

<div class="photo-card">
    <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
    <div class="info">
        <p class="info-item">
            <b class="info-item--name">Likes</b>
            <b>{{likes}}</b>
        </p>
        <p class="info-item">
            <b class="info-item--name">Views</b>
            <b>{{views}}</b>
        </p>
        <p class="info-item">
            <b class="info-item--name">Comments</b>
            <b>{{comments}}</b>
        </p>
        <p class="info-item">
            <b class="info-item--name">Downloads</b>
            <b>{{downloads}}</b>
        </p>
    </div>
</div>

{{/each}}